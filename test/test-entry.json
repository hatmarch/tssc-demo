{
    "kind": "rekord",
    "apiVersion": "0.0.1",
    "spec": {
        "signature": {
            "format": "pgp",
            "content": "%SIG%",
            "publicKey": {
                "content":"%PUB%"
             }
        },
        "data": {
            "content": "%ART%",
            "hash": {
                "algorithm": "sha256",
                "value": "%HASH%"
            }
        },
        "extraData": "%ART%"
    }
}
